<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="WindowBackgroundColor">#1F0510</Color>
    <Color x:Key="PanelBackgroundColor">#330A1C</Color>
    <Color x:Key="PrimaryTextColor">#FFD1DC</Color>
    <Color x:Key="SecondaryTextColor">#D16D98</Color>

    <Color x:Key="ButtonNormalColor">#4A0E28</Color>
    <Color x:Key="ButtonHoverColor">#751840</Color>
    <Color x:Key="ButtonPressedColor">#9E2257</Color>

    <Color x:Key="AccentColor">#FF1493</Color>

    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="{StaticResource WindowBackgroundColor}"/>
    <SolidColorBrush x:Key="PanelBackgroundBrush" Color="{StaticResource PanelBackgroundColor}"/>
    <SolidColorBrush x:Key="PrimaryTextBrush" Color="{StaticResource PrimaryTextColor}"/>
    <SolidColorBrush x:Key="ButtonNormalBrush" Color="{StaticResource ButtonNormalColor}"/>
    <SolidColorBrush x:Key="ButtonHoverBrush" Color="{StaticResource ButtonHoverColor}"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>
</ResourceDictionary>